{
    "name": "Ecoquest Game",
    "description": "Ecoquest Options Game made with Phaser",
    "version": "1.3.2",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/verina09/ecoquest.git"
    },
